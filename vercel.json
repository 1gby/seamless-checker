{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/converter",
      "destination": "/converter.html"
    },
    {
      "source": "/converter/:path*",
      "destination": "/converter.html"
    },
    {
      "source": "/seamless",
      "destination": "/seamless.html"
    },
    {
      "source": "/seamless/:path*",
      "destination": "/seamless.html"
    },
    {
      "source": "/watcher",
      "destination": "/watcher.html"
    },
    {
      "source": "/watcher/:path*",
      "destination": "/watcher.html"
    }
  ]
}